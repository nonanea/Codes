%%%%%%%%%%%%%%%%% NormFactor %%%%%%%%%%%%%%%%%%

NormFactor: "SigXsecOverSM"
  Title: "SigXsecOverSM"
  Constant: TRUE
  Nominal:1
  Min:  -50
  Max:  50
  Samples: ggF
  %Samples: ggF,VBF
  Regions: SR

NormFactor: "mu_ttXsec"
  Title: "mu_tt"
  Nominal:1
  Min:  -50
  Max:  50
  %Samples: tt_ttbar, ttV_ttbar, VVHiggs_ttbar, Zjets_ttbar, VR_ttbar
  Samples: *ttbar
  %Regions: all
  %Exclude: SR

NormFactor: "mu_ttVXsec"
  Title: "mu_ttV"
  Nominal:1
  Min:  -50
  Max:  50
  %Samples: tt_ttV ,ttV_ttV, VVHiggs_ttV, Zjets_ttV, VR_ttV
  Samples: *ttV
  %Regions:ttV_CR, tt_CR, SR
  %Exclude: SR

NormFactor: "mu_VVXsec"
  Title: "mu_VV"
  Nominal:1
  Min:  -50
  Max:  50
  %Samples: tt_VV, ttV_VV, VVHiggs_VV, Zjets_VV, VR_VV
  Samples: *VV
  %Regions:VVHiggs_CR
  %Exclude: SR

NormFactor: "mu_HiggsXsec"
  Title: "mu_Higgs"
  Nominal:1
  Min:  -50
  Max:  50
  %Samples: VVHiggs_Higgs, VR_Higgs
  Samples: *Higgs
  %Regions:VVHiggs_CR, SR
  %Exclude: SR

NormFactor: "mu_ZjetsXsec"
  Title: "mu_Zjets"
  Nominal:1
  Min:  -50
  Max:  50
  %Samples: Zjets_Zjets, VR_Zjets
  Samples: *Zjets
  %Regions:Zjets_CR, SR
  %Exclude: SR

%%%%%Systematic%%%%%%

Systematic: "ttXsec"
