Job: "bb4l_CRFit"
  Label: "bb4l_CR Fit"
  CmeLabel: "13 TeV"
  LumiLabel: "139 fb^{-1}"
  %POI: "SigXsecOverSM","mu_ttXsec","mu_ttVXsec","mu_VVXsec","mu_HiggsXsec","mu_ZjetsXsec"
  POI: "SigXsecOverSM"
  ReadFrom: NTUP
  %NtuplePath: "bb4lInputs/test/"
  NtuplePath: "/Lab/Di-Higgs/Analysis/v10samples/BDT/Results/v1/"
  NtupleName: "nominal"
  MCweight: "weight"
  DebugLevel: 10
  GuessMCStatEmptyBins: TRUE
  SystControlPlots: TRUE
  UseGammaPulls: TRUE
  BlindSRs: TRUE
  PlotOptions: YIELDS,NORMSIG
  %GetChi2: TRUE
  ImageFormat: png,eps,svg
  RatioYmax: 1.8
  RatioYmin: 0.2
  RankingMaxNP: 30
  NPRankingCanvasSize: 3500, 1400
  %PruningType: COMBINEDREFERENCE
  %RankingPlot: Systs

Fit: "myFit"
  FitType: SPLUSB
  FitRegion: CRSR
  %FitBlind: TRUE
  %doLHscan: SigXsecOverSM
  %LHscanSteps: 5
  POIAsimov: 1
  UseMinos: SigXsecOverSM

Limit: "limit"
  LimitType: ASYMPTOTIC
  %SignalInjection: TRUE
  %LimitBlind: TRUE

%Significance: "limit"
  %SignificanceBlind: TRUE
  %POI: "SigXsecOverSM"
  %POIAsimov: 1

%%%%%%%%%%% tt CR definition %%%%%%%%%%%
Region: "tt_CR"
  Label: "t#bar{t}"
  Type: CONTROL
  Variable: "ttBDT",8,-1.0,1.0
  VariableTitle: "BDT"
  %Binning:-1,1
  %DataType: ASIMOV
  DataType: DATA
    
Sample: "tt_ttbar"
  Type: BACKGROUND
  Title: "t#bar{t}"
  FillColor: 3
  LineColor: 1
  NtupleFile: "tt/tt"
  Regions: "tt_CR"

Sample: "tt_VV"
  Type: BACKGROUND
  Title: "VV"
  FillColor: 4
  LineColor: 1
  NtupleFile: "tt/VV"
  Regions: "tt_CR"

Sample: "tt_ttV"
  Type: BACKGROUND
  Title: "ttV"
  FillColor: 5
  LineColor: 1
  NtupleFile: "tt/ttV"
  Regions: "tt_CR"
    
Sample: "tt_Higgs"
